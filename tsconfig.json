{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["DOM", "DOM.Iterable"],
    // "jsx": "react",
    "jsx": "react-jsx",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "allowJs": true,
    "esModuleInterop": true,
    "outDir": "./build"
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/components/**/*.d.ts",
    "src/components/Hamburger.tsx",
    "node_modules/react-scripts/lib/react-app.d.ts"
  ],
  "exclude": ["propTypes.d.ts"]
}
